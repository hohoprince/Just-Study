            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/mobile_navigation"
            app:startDestination="@+id/navigation_home">

    <fragment
            android:id="@+id/navigation_home"
            android:name="com.sunhoon.juststudy.ui.home.HomeFragment"
            android:label="@string/title_home"
            tools:layout="@layout/fragment_home"/>

    <fragment
            android:id="@+id/navigation_study"
            android:name="com.sunhoon.juststudy.ui.study.StudyFragment"
            android:label="@string/title_study"
            tools:layout="@layout/fragment_study"/>

    <fragment
            android:id="@+id/navigation_settings"
            android:name="com.sunhoon.juststudy.ui.settings.SettingsFragment"
            android:label="@string/title_settings"
            tools:layout="@layout/fragment_settings"/>
